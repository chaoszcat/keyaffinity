{
  "name": "KeyAffinity",
  "version": "1.1.5",
  "manifest_version": 2,
  "description": "Adds keyboard navigation functions to FurAffinity",
  "content_scripts": [
    {
      "matches": ["http://www.furaffinity.net/*"],
      "js": ["jquery.js", "hotkeys.js", "background.js"]
    }
  ],
  "icons": { "128": "icon.png" },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}